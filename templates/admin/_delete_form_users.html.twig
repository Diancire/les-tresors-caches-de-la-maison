<form method="post" action="{{ path('app_admin_delete', {'id': user.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');" class="display-inline-block">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn_delete"><i class="fa-solid fa-trash"></i></button>
</form>
